    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "concurrently": "^7.5.0",
    "copy-and-watch": "^0.1.6",
    "countries-list": "^2.6.1",
    "draft-convert": "^2.1.13",
    "draft-js": "^0.11.7",
