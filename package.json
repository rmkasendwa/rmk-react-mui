    "react-file-icon": ">=1.3.0",
    "react-intersection-observer": ">=9.4.0",
    "react-router-dom": ">=6.4.3"
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  }
}
