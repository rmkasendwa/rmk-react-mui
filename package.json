  "license": "MIT",
  "author": "Ronald M. Kasendwa",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "prebuild": "rm -rf lib",
    "build": "npm run build:esm && npm run build:cjs",
    "postbuild": "node ./scripts/postbuild",
    "build-storybook": "storybook build -o docs",
    "build:cjs": "tsc --project tsconfig-cjs.compile.json && copy-and-watch src/**/*.{png,jpg,css,scss,sass,json} lib",
    "build:esm": "tsc && copy-and-watch src/**/*.{png,jpg,css,scss,sass,json} lib",
    "eslint": "eslint --fix",
    "format": "prettier --write \"./**/*.{js,jsx,ts,tsx,html,htm,json,css,scss,md}\"",
    "publish": "npm run build && npm publish ./lib/ --access=public",
    "snippets:update": "ts-node --project tsconfig-cjs.compile.json ./snippets/update-snippets",
    "storybook": "storybook dev -p 6006",
    "tsc": "tsc"
  },
