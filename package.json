{
  "name": "@infinite-debugger/react-mui",
  "version": "0.5.0-alpha.24",
  "description": "RMK React MUI is a React UI library of utility components, hooks and contexts",
  "keywords": [
    "react",
    "react-component",
    "mui",
    "material-ui",
    "material design"
  ],
  "homepage": "https://github.com/rmkasendwa/rmk-react-mui#readme",
  "bugs": {
    "url": "https://github.com/rmkasendwa/rmk-react-mui/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rmkasendwa/rmk-react-mui.git"
  },
  "license": "MIT",
  "author": "Ronald M. Kasendwa",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "prebuild": "rm -rf lib",
    "build": "npm run build:esm && npm run build:cjs",
    "postbuild": "node ./scripts/postbuild",
    "build-storybook": "storybook build -o docs",
    "build:cjs": "tsc --project tsconfig-cjs.compile.json && copy-and-watch src/**/*.{png,jpg,css,scss,sass,json} lib",
    "build:esm": "tsc && copy-and-watch src/**/*.{png,jpg,css,scss,sass,json} lib",
    "eslint": "eslint --fix",
    "format": "prettier --write \"./**/*.{js,jsx,ts,tsx,html,htm,json,css,scss,md}\"",
    "postinstall": "npm run prepare",
    "prepare": "husky install",
    "publish": "npm run build && npm publish ./lib/ --access=public",
    "start": "concurrently --names \"Typescript,Assets\" -c \"blue.bold,magenta.bold\" \"tsc -w\" \"copy-and-watch --watch src/**/*.{png,jpg,css,scss,sass,json} lib\"",
    "storybook": "storybook dev -p 6006",
    "test": "react-scripts test --watchAll=false",
    "test:watch": "react-scripts test",
    "tsc": "tsc"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "dependencies": {
    "clsx": "^1.2.1",
    "object-hash": "^3.0.0",
    "react-merge-refs": "^2.0.1",
    "react-smooth-dnd": "^0.11.1"
  },
  "devDependencies": {
    "@draft-js-plugins/editor": "^4.1.3",
    "@draft-js-plugins/image": "^4.1.3",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@infinite-debugger/rmk-js-extensions": "^0.1.0-alpha.13",
    "@infinite-debugger/rmk-utils": "^0.1.0-alpha.12",
    "@mui/base": "^5.0.0-alpha.114",
    "@mui/icons-material": "^5.10.9",
    "@mui/lab": "^5.0.0-alpha.106",
    "@mui/material": "^5.10.12",
    "@mui/styles": "^5.10.10",
    "@react-theming/storybook-addon": "^1.1.10",
    "@storybook/addon-actions": "^7.0.6",
    "@storybook/addon-essentials": "^7.0.6",
    "@storybook/addon-links": "^7.0.6",
    "@storybook/addon-mdx-gfm": "^7.0.6",
    "@storybook/node-logger": "^7.0.6",
    "@storybook/preset-create-react-app": "^7.0.6",
    "@storybook/react": "^7.0.6",
    "@storybook/react-webpack5": "^7.0.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/crypto-js": "^4.1.1",
    "@types/draft-convert": "^2.1.4",
    "@types/draft-js": "^0.11.10",
    "@types/google-libphonenumber": "^7.4.23",
    "@types/jest": "^29.2.1",
    "@types/lodash": "^4.14.187",
    "@types/node": "^18.11.9",
    "@types/object-hash": "^2.2.1",
