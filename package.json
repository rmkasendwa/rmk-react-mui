{
  "name": "@infinite-debugger/react-mui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build-storybook": "build-storybook -s public",
    "storybook": "start-storybook -p 6006 -s public",
    "prebuild": "rm -rf lib",
    "build": "tsc && copy-and-watch src/**/*.{png,jpg,css,scss,sass,json} lib",
    "postbuild": "cp package.json lib/package.json",
    "start": "concurrently --names \"Typescript,Assets\" -c \"blue.bold,magenta.bold\" \"tsc -w\" \"copy-and-watch --watch src/**/*.{png,jpg,css,scss,sass,json} lib\"",
    "test": "jest"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "dependencies": {
    "@react-theming/storybook-addon": "^1.1.3",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^16.11.21",
    "@types/object-hash": "^2.2.1",
    "@types/react": "^17.0.38",
    "concurrently": "^7.0.0",
    "copy-and-watch": "^0.1.6",
    "crypto-js": "^4.1.1",
    "date-fns": "^2.28.0",
    "datejs": "^1.0.0-rc3",
    "google-libphonenumber": "^3.2.26",
    "jest": "^27.4.7",
    "object-hash": "^2.2.0",
    "query-string": "^7.1.0",
    "react": "^17.0.2",
    "react-number-format": "^4.9.1",
    "react-redux": "^7.2.6",
    "redux-thunk": "^2.4.1",
    "sass-loader": "^12.4.0",
    "style-loader": "^3.3.1",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5",
    "underscore": "^1.13.2"
  },
  "devDependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.3.1",
    "@mui/lab": "^5.0.0-alpha.66",
    "@mui/material": "^5.3.1",
    "@mui/styles": "^5.3.0",
    "@storybook/addon-actions": "^6.4.14",
    "@storybook/addon-essentials": "^6.4.14",
    "@storybook/addon-links": "^6.4.14",
    "@storybook/builder-webpack5": "^6.4.14",
    "@storybook/manager-webpack5": "^6.4.14",
    "@storybook/node-logger": "^6.4.14",
    "@storybook/preset-create-react-app": "^4.0.0",
    "@storybook/react": "^6.4.14",
    "@types/crypto-js": "^4.1.0",
    "@types/datejs": "0.0.32",
    "@types/google-libphonenumber": "^7.4.23",
    "@types/react-dom": "^17.0.11",
    "@types/underscore": "^1.11.4",
    "axios": "^0.25.0",
    "formik": "^2.2.9",
    "node-sass": "^7.0.1",
    "webpack": "^5.67.0"
  }
}