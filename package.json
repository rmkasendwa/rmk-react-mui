{
  "name": "@infinite-debugger/react-mui",
  "version": "0.1.0-alpha.4",
  "description": "<h1 align=\"center\">infinite-debugger/react-mui</h1>",
  "keywords": [
    "react",
    "react-component",
    "mui",
    "material-ui",
    "material design"
  ],
  "homepage": "https://github.com/rmkasendwa/rmk-react-mui#readme",
  "bugs": {
    "url": "https://github.com/rmkasendwa/rmk-react-mui/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rmkasendwa/rmk-react-mui.git"
  },
  "license": "MIT",
  "author": "Ronald M. Kasendwa",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "prebuild": "rm -rf lib",
    "build": "npm run tsc && copy-and-watch src/**/*.{png,jpg,css,scss,sass,json} lib",
    "postbuild": "node ./scripts/postbuild",
    "build-storybook": "build-storybook -s public",
    "eslint": "eslint --fix",
    "format": "prettier --write \"./**/*.{js,jsx,ts,tsx,html,htm,json,css,scss,md}\"",
    "postinstall": "npm run prepare",
    "prepare": "husky install",
    "start": "concurrently --names \"Typescript,Assets\" -c \"blue.bold,magenta.bold\" \"tsc -w\" \"copy-and-watch --watch src/**/*.{png,jpg,css,scss,sass,json} lib\"",
    "storybook": "start-storybook -p 6006 -s public",
    "test": "react-scripts test --watchAll=false",
    "tsc": "tsc"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "dependencies": {
    "axios": "^0.25.0",
    "crypto-js": "^4.1.1",
    "date-fns": "^2.28.0",
    "datejs": "^1.0.0-rc3",
    "google-libphonenumber": "^3.2.26",
    "object-hash": "^2.2.0",
    "query-string": "^7.1.0",
    "react-number-format": "^4.9.1",
    "underscore": "^1.13.2"
  },
  "devDependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.3.1",
    "@mui/lab": "^5.0.0-alpha.66",
    "@mui/material": "^5.3.1",
    "@mui/styles": "^5.3.0",
    "@react-theming/storybook-addon": "^1.1.3",
    "@storybook/addon-actions": "^6.4.14",
    "@storybook/addon-essentials": "^6.4.14",
    "@storybook/addon-links": "^6.4.14",
    "@storybook/builder-webpack5": "^6.4.14",
    "@storybook/manager-webpack5": "^6.4.14",
    "@storybook/node-logger": "^6.4.14",
    "@storybook/preset-create-react-app": "^4.0.0",
    "@storybook/react": "^6.4.14",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/crypto-js": "^4.1.0",
    "@types/datejs": "0.0.32",
    "@types/google-libphonenumber": "^7.4.23",
    "@types/jest": "^27.4.0",
    "@types/node": "^16.11.21",
    "@types/object-hash": "^2.2.1",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/underscore": "^1.11.4",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "concurrently": "^7.0.0",
    "copy-and-watch": "^0.1.6",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "formik": "^2.2.9",
    "fs-extra": "^10.0.0",
    "husky": "^7.0.4",
    "import-sort-style-module": "^6.0.0",
    "node-sass": "^7.0.1",
    "prettier": "^2.5.1",
    "prettier-plugin-import-sort": "0.0.7",
    "prettier-plugin-sort-json": "0.0.2",
    "react": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-scripts": "^5.0.0",
    "redux-thunk": "^2.4.1",
    "sass-loader": "^12.4.0",
    "style-loader": "^3.3.1",
    "typescript": "^4.5.5",
    "webpack": "^5.67.0"
  },
  "peerDependencies": {
    "@emotion/react": ">=11.7.1",
    "@emotion/styled": ">=11.6.0",
    "@mui/icons-material": ">=5.3.1",
    "@mui/lab": ">=5.0.0-alpha.66",
    "@mui/material": ">=5.3.1",
    "@mui/styles": ">=5.3.0",
    "react": ">=17.0.2",
    "react-redux": ">=7.2.6",
    "redux-thunk": ">=2.4.1"
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  }
}
